<script>


//광주 바깥쪽 폴리곤 생성
$.getJSON("./assets/json/gj_line.geojson", function(geojson) {
	 
   var data_out = geojson.features;
   var coordinates_out = []; 
   var name_out = ''; 

   $.each(data_out, function(index, val) {

       coordinates_out = val.geometry.coordinates;
